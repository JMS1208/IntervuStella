<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:id="@+id/root"
    >
    <eightbitlab.com.blurview.BlurView xmlns:android="http://schemas.android.com/apk/res/android"
        android:orientation="vertical"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/btm_rounded_corner_background"
        android:id="@+id/middle_item_blur_view"
        >
        <androidx.compose.ui.platform.ComposeView
            android:id="@+id/middle_item_compose_view"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            />
    </eightbitlab.com.blurview.BlurView>
</FrameLayout>
